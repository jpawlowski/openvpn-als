<%@ page import="com.adito.install.forms.SelectCertificateSourceForm" %>
<%@ taglib uri="http://struts.apache.org/tags-html" prefix="html" %>
<%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean" %>
<%@ taglib uri="http://struts.apache.org/tags-logic" prefix="logic" %>

<table border="0" cellpadding="0" cellspacing="0">
	<tr class="formText">
		<td class="label">
			<div class="required"><bean:message key="installation.createNewCertificate.hostname" bundle="install"/></div>
		</td>
		<td class="value">
			<html:text property="hostname" size="30"/>
		</td>
	</tr>
	<tr class="formText">
		<td class="label">
			<div class="required"><bean:message key="installation.createNewCertificate.organisationalUnit" bundle="install"/></div>
		</td>
		<td class="value">
			<html:text property="organisationalUnit" size="20"/>
		</td>
	</tr>
	<tr class="formText">
		<td class="label">
			<div class="required"><bean:message key="installation.createNewCertificate.company" bundle="install"/></div>
		</td>
		<td class="value">
			<html:text property="company" size="30"/>
		</td>
	</tr>
	<tr class="formText">
		<td class="label">
			<div class="required"><bean:message key="installation.createNewCertificate.city" bundle="install"/></div>
		</td>
		<td class="value">
			<html:text property="city" size="30"/>
		</td>
	</tr>
	<tr class="formText">
		<td class="label">
			<div class="required"><bean:message key="installation.createNewCertificate.state" bundle="install"/></div>
		</td>
		<td class="value">
			<html:text property="state" size="20"/>
		</td>
	</tr>
	<tr class="formText">
		<td class="label">
			<div class="required"><bean:message key="installation.createNewCertificate.countryCode" bundle="install"/></div>
		</td>
		<td class="value">
			<html:text property="countryCode" size="5" maxlength="3"/>
		</td>
	</tr>
</table>